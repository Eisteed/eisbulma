  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch:editor\"",
    "watch:editor": "sass --watch src/styles/gutenberg-editor-bulma.scss:src/styles/gutenberg-editor-bulma.css --no-source-map --load-path=node_modules",
    "build:editor": "sass src/styles/gutenberg-editor-bulma.scss:src/styles/gutenberg-editor-bulma.css --no-source-map --load-path=node_modules",
    "build": "cross-env NODE_ENV=production vite build",
    "upload": "node upload.mjs",
    "deploy": "npm run build:editor && npm run build && npm run upload",
    "preview": "vite preview --host"
  },
